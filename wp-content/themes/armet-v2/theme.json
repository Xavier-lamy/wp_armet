{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": false,
        "border": {
            "radius": false,
            "color": false,
            "style": false,
            "width": false
        },
        "color": {
            "custom": true,
            "customDuotone": true,
            "customGradient": true,
            "duotone": [
                {
                    "colors": [ "#000", "#FFF" ],
                    "slug": "black-and-white",
                    "name": "Black and White"
                }
            ],
            "gradients": [
                {
                    "slug": "gradient-black",
                    "gradient": "linear-gradient(180deg,#1E1E1E,#000)",
                    "name": "Black gradient"
                },
                {
                    "slug": "gradient-brown",
                    "gradient": "linear-gradient(80deg,#301b0744,#301b07)",
                    "name": "Brown gradient"
                }
            ],
            "link": false,
            "palette": [
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#000000"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#ffffff"
                },
                {
                    "name": "Primary",
                    "slug": "primary",
                    "color": "#452b17"
                },
                {
                    "name": "Secondary",
                    "slug": "secondary",
                    "color": "#724b2f"
                },
                {
                    "name": "Tertiary",
                    "slug": "tertiary",
                    "color": "#301b07"
                },
                {
                    "name": "Transparent tertiary",
                    "slug": "tertiary-transparent",
                    "color": "#301b0744"
                },
                {
                    "name": "Button color",
                    "slug": "button-color",
                    "color": "#294673"
                },
                {
                    "name": "Button hover",
                    "slug": "button-hover",
                    "color": "#213e69"
                },
                {
                    "name": "Background",
                    "slug": "theme-background",
                    "color": "#1E1E1E"
                },                {
                    "name": "Text",
                    "slug": "theme-text",
                    "color": "#ddd"
                }
            ],
            "text": true,
            "background": true,
            "defaultGradients": false,
            "defaultPalette": false
        },
        "custom": {},
        "layout": {
            "contentSize": "800px",
            "wideSize": "1000px"
        },
        "spacing": {
            "margin": false,
            "padding": false,
            "blockGap": null,
            "units": [ "px", "em", "rem", "vh", "vw" ]
        },
        "typography": {
            "customFontSize": true,
            "lineHeight": false,
            "dropCap": true,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "textDecoration": true,
            "textTransform": true,
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "0.72rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "0.8rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "1rem",
                    "name": "Normal"
                },
                {
                    "slug": "x-large",
                    "size": "1.1rem",
                    "name": "Large"
                },
                {
                    "slug": "h6",
                    "size": "1.2rem",
                    "name": "Size h6"
                },
                {
                    "slug": "h5",
                    "size": "1.4rem",
                    "name": "Size h5"
                },
                {
                    "slug": "h4",
                    "size": "1.8rem",
                    "name": "Size h4"
                },
                {
                    "slug": "h3",
                    "size": "2.2rem",
                    "name": "Size h3"
                },
                {
                    "slug": "h2",
                    "size": "2.6rem",
                    "name": "Size h2"
                },
                {
                    "slug": "h1",
                    "size": "3rem",
                    "name": "Size h1"
                }
            ], 
            "fontFamilies": [
                {
                    "fontFamily": "Montserrat, sans-serif",
                    "slug": "system-font",
                    "name": "Montserrat"
                },
                {
                    "fontFamily": "Domine, Arial, sans-serif",
                    "slug": "title-font",
                    "name": "Domine"
                }
            ]
        },
        "blocks": {
            "core/paragraph": {
                "color": {},
                "custom": {},
                "layout": {},
                "spacing": {},
                "typography": {}
            },
            "core/heading": {}
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--theme-background)",
            "text": "var(--wp--preset--color--theme-text)"
        },
        "typography": {
            "fontSize": "var(--wp--preset--font-size--large)",
            "lineHeight": "2rem"
        },
        "elements": {
            "link": {
                "border": {},
                "color": {
                    "text": "var(--wp--preset--color--theme-text)"
                },
                "spacing": {},
                "typography": {}
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h1)",
                    "fontFamily": "var(--wp--preset--font-family--title-font)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h2)",
                    "fontFamily": "var(--wp--preset--font-family--title-font)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h3)",
                    "fontFamily": "var(--wp--preset--font-family--title-font)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h4)",
                    "fontFamily": "var(--wp--preset--font-family--title-font)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h5)",
                    "fontFamily": "var(--wp--preset--font-family--title-font)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h6)",
                    "fontFamily": "var(--wp--preset--font-family--title-font)"
                }
            }
        },
        "blocks": {
            "core/group": {
                "border": {},
                "color": {},
                "spacing": {},
                "typography": {},
                "elements": {
                    "link": {},
                    "h1": {},
                    "h2": {},
                    "h3": {},
                    "h4": {},
                    "h5": {},
                    "h6": {}
                }
            }
        }
    }
}
